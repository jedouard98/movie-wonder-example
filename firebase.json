// Link to routing documentation: https://firebase.google.com/docs/hosting/full-config
{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],

    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ],

    "cleanUrls": true
  }
}
